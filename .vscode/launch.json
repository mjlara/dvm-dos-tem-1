    "version": "0.2.0",
    "configurations": [
        { 
            "name": "(gdb) dvmdostem",
            "type": "cppdbg",
            "request": "launch",
            "program": "/work/dvmdostem",
            "args": ["--force-cmt", "15", 
                     "--log-level", "fatal", 
                     "-e", "100", 
                     "-s", "300", 
                     "-p", "100",
                     "-t", "122", 
                     "-n", "0"],
            "stopAtEntry": false,
            "cwd": "/data/workflows/BONA-black-spruce-fire-1930/",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}